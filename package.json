{
	"name": "tasks-shell-input",
	"displayName": "Tasks Shell Input",
	"description": "Use shell commands as input for your tasks",
	"version": "1.6.0",
	"publisher": "augustocdias",
	"repository": {
		"url": "https://github.com/augustocdias/vscode-shell-command"
	},
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:shellCommand.execute"
	],
	"main": "./out/extension.js",
	"contributes": {
		"menus": {
			"commandPalette": [
				{
					"command": "shellCommand.execute",
					"when": "false"
				}
			]
		},
		"commands": [
			{
				"command": "shellCommand.execute",
				"title": "Execute shell command"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
